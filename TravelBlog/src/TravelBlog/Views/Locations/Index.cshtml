@model IEnumerable<TravelBlog.Models.Location>
@{
    ViewData["Title"] = "Travel Blog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Locations</h1>

@foreach(var location in Model)
{
    <p>@Html.DisplayFor(modelLocation => location.Name) | @Html.ActionLink("Edit", "Edit", new { id = location.LocationId }) </p>
}

<p>@Html.ActionLink("HOME", "Index", "Home")</p>
<p>@Html.ActionLink("ADD NEW LOCATION", "Create")</p>